{
	"name": "context-layer",
	"version": "0.0.1",
	"type": "module",
	"description": "Transform any knowledge base or documentation into AI-ready data for RAG and fine-tuning",
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.30.0",
		"apify": "^3.5.2",
		"cheerio": "^1.0.0-rc.12",
		"crawlee": "^3.11.0",
		"openai": "^4.70.0",
		"tiktoken": "^1.0.22"
	},
	"devDependencies": {
		"@apify/eslint-config": "^1.0.0",
		"@apify/tsconfig": "^0.1.1",
		"@eslint/js": "^9.39.2",
		"@types/node": "^22.15.32",
		"@vitest/coverage-v8": "^4.0.16",
		"eslint": "^9.29.0",
		"tsx": "^4.20.3",
